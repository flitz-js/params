{
  "name": "@flitz/params",
  "version": "2.0.1",
  "description": "Extracts path parameters and makes them available in a flitz request.",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "index.d.ts",
    "lib/**/*",
    "!lib/sandbox/**/*",
    "!**/*.map"
  ],
  "scripts": {
    "build": "del ./lib && tsc -p ./tsconfig.json",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "ts-node-dev --respawn ./src/sandbox/index.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flitz-js/params.git"
  },
  "keywords": [
    "flitz",
    "params",
    "parameters",
    "path",
    "parser",
    "key value",
    "object",
    "typescript"
  ],
  "author": "Marcel Joachim Kloubert <marcel.kloubert@gmx.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flitz-js/params/issues"
  },
  "homepage": "https://github.com/flitz-js/params#readme",
  "devDependencies": {
    "@flitz/tsconfig": "0.1.1",
    "@types/jest": "^26.0.7",
    "@types/node": "^10.17.28",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-flitz": "0.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.0.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-unicorn": "^21.0.0",
    "flitz": "0.14.0",
    "jest": "^26.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "regexparam": "^1.3.0"
  }
}
